{
  "name": "filterable-mikroorm-app",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "NODE_ENV=test NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --runInBand",
    "test:watch": "NODE_ENV=test NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --watch",
    "start": "tsx src/index.ts",
    "codegen:filters": "tsx scripts/generate-filter-queries.ts",
    "codegen:filters:microorm": "FILTER_BUILDERS=mikroorm DEBUG_FILTER_CODEGEN=1 npm run codegen:filters",
    "codegen:filters:prisma": "FILTER_BUILDERS=prisma DEBUG_FILTER_CODEGEN=1 npm run codegen:filters",
    "codegen:filters:knex": "FILTER_BUILDERS=knex DEBUG_FILTER_CODEGEN=1 npm run codegen:filters",
    "codegen:filters:microorm:ci": "HYGEN_OVERWRITE=1 FILTER_BUILDERS=mikroorm DEBUG_FILTER_CODEGEN=1 npm run codegen:filters",
    "codegen:filters:prisma:ci": "HYGEN_OVERWRITE=1 FILTER_BUILDERS=prisma DEBUG_FILTER_CODEGEN=1 npm run codegen:filters",
    "codegen:filters:knex:ci": "HYGEN_OVERWRITE=1 FILTER_BUILDERS=knex DEBUG_FILTER_CODEGEN=1 npm run codegen:filters",
    "db:microorm:reset": "tsx scripts/microorm/db-reset.ts",
    "db:microorm:seed": "tsx scripts/microorm/db-seed.ts",
    "db:microorm:reseed": "npm run db:microorm:reset && npm run db:microorm:seed",
    "smoke:users:microorm": "tsx scripts/microorm/smoke-users.ts",
    "db:prisma:generate": "prisma generate",
    "db:prisma:format": "prisma format",
    "db:prisma:migrate": "prisma migrate dev",
    "db:prisma:studio": "prisma studio",
    "db:prisma:push": "npx prisma db push",
    "db:prisma:reset": "tsx scripts/prisma/db-reset.ts",
    "db:prisma:seed": "tsx scripts/prisma/db-seed.ts",
    "db:prisma:reseed": "npm run db:prisma:reset && npm run db:prisma:seed",
    "smoke:users:prisma": "tsx scripts/prisma/smoke-users.ts",
    "db:knex:seed": "tsx scripts/knex/db-seed.ts",
    "db:knex:reset": "tsx scripts/knex/db-reset.ts",
    "db:knex:reseed": "npm run db:knex:reset && npm run db:knex:seed",
    "smoke:users:knex": "tsx scripts/knex/smoke-users.ts",
    "server:run": "tsx src/index.ts",
    "server:dropSchema": "tsx src/generateOpenAPI.ts > openapi.yaml"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/http-errors": "^2.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@types/supertest": "^6.0.3",
    "hygen": "^6.2.11",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@mikro-orm/core": "^6.3.6",
    "@mikro-orm/sqlite": "^6.5.8",
    "@prisma/client": "^6.18.0",
    "better-sqlite3": "^12.4.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-zod-api": "^25.5.3",
    "http-errors": "^2.0.0",
    "knex": "^3.1.0",
    "prisma": "^6.18.0",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.4.5",
    "zod": "^4.1.12"
  },
  "imports": {
    "#/*": "./src/*"
  }
}
