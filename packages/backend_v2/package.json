{
  "name": "filterable-mikroorm-app",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "codegen:filters": "tsx scripts/generate-filter-queries.ts",
    "codegen:filters:microorm": "FILTER_BUILDERS=mikroorm DEBUG_FILTER_CODEGEN=1 npm run codegen:filters",
    "codegen:filters:prisma": "FILTER_BUILDERS=prisma DEBUG_FILTER_CODEGEN=1 npm run codegen:filters",
    "db:microorm:reset": "tsx scripts/microorm/db-reset.ts",
    "db:microorm:seed": "tsx scripts/microorm/db-seed.ts",
    "db:microorm:reseed": "npm run db:microorm:reset && npm run db:microorm:seed",
    "smoke:users:microorm": "tsx scripts/microorm/smoke-users.ts",
    "db:prisma:generate": "prisma generate",
    "db:prisma:format": "prisma format",
    "db:prisma:migrate": "prisma migrate dev",
    "db:prisma:studio": "prisma studio",
    "db:prisma:push": "npx prisma db push",
    "db:prisma:reset": "tsx scripts/prisma/db-reset.ts",
    "db:prisma:seed": "tsx scripts/prisma/db-seed.ts",
    "db:prisma:reseed": "npm run db:prisma:reset && npm run db:prisma:seed",
    "smoke:users:prisma": "tsx scripts/prisma/smoke-users.ts"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^22.7.5",
    "hygen": "^6.2.11",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@mikro-orm/core": "^6.3.6",
    "@mikro-orm/sqlite": "^6.5.8",
    "@prisma/client": "^6.18.0",
    "better-sqlite3": "^12.4.1",
    "express": "^5.1.0",
    "prisma": "^6.18.0",
    "reflect-metadata": "^0.2.2"
  }
}
